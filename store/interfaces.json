{
  "comparison-context-wrapper": {
    "component": "ProductComparisonWrapper"
  },
  "product-comparison": {
    "around": ["comparison-context-wrapper"],
    "allowed": ["product-comparison-block"]
  },
  "product-comparison-block": {},
  
  "product-comparison-block.selector": {
    "component": "ProductSelector"
  },
  
  "product-comparison-block.grid-row": {
    "component": "ComparisonGridRow"
  },


  "product-comparison-drawer": {
    "allowed": ["list-context.comparison-list"],
    "component": "ComparisonDrawer"
  },
  "list-context.comparison-list": {
    "allowed": ["product-summary"],
    "composition": "children",
    "component": "ProductSummaryList"
  },


  "comparison-page": {
    "component": "ComparisonPage",
    "composition": "children"
  },
  "product-comparison-list": {
    "component": "ComparisonList",
    "allowed": ["product-comparison-block", "list-context.comparison-list"]
  },
  "product-comparison-block.product-specifications": {
    "allowed": ["list-context.comparison-row-list"],
    "component": "ComparisonGrid"
  },
  "list-context.comparison-row-list": {
    "composition": "children",
    "component": "ComparisonGridRowContent"
  }

}
